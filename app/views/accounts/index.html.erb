<p style="color: green"><%= notice %></p>

<h1>Accounts</h1>

<div id="accounts">
  <% @accounts.each do |account| %>
    <%= render account %>
    <p>
      <%= link_to "Ver conta", account,  class: "btn btn-primary" %>
    </p>
  <% end %>
</div>

<%= link_to "New account", new_account_path ,  class: "btn btn-primary" %>
